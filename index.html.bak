	<script> 
		function updater() { 
			$.get(
				"newImage",
				{},
				function(data) {
					$(".avatar").css('background-image', "url(" + data[0] + ")");
					document.getElementById("leftImg").src = data[1];
					document.getElementById("rightImg").src = data[2];
				}
			);
		};
	</script>
	
